    "mocha": "^10.2.0",
    "wdio-video-reporter": "^3.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/register": "^7.16.5",
    "@wdio/browserstack-service": "^7.16.12",
    "@wdio/cli": "^7.16.12",
    "@wdio/local-runner": "^7.16.12",
    "@wdio/mocha-framework": "^7.16.11",
    "chromedriver": "117.0.0",
    "eslint": "^7.25.0",
